# Environment Configuration
NODE_ENV="development" # Options: 'development', 'production'
PORT="8080"            # The port your server will listen on
HOST="localhost"       # Hostname for the server

# CORS Settings
CORS_ORIGIN="http://localhost:8080" # Allowed CORS origin, adjust as necessary

# Rate Limiting
RATE_LIMIT_WINDOW_MS="1000"  # Window size for rate limiting (ms)
RATE_LIMIT_MAX_REQUESTS="20" # Max number of requests per window per IP

# LOG CONFIG
LOG_LEVEL=debug
LOG_FORMAT=text

# DYNAMIC
DYNAMIC_AUTH_TOKEN=your_auth_token_here
DYNAMIC_ENVIRONMENT_ID=your_environment_id_here

# MONGO DB
MONGODB_URI=